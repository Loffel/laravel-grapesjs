{
    "name": "loffel/laravel-grapesjs",
    "description": "A package to easily integrate GrapesJS into your laravel project.",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": ">=5.6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dotlogics",
            "email": "hello@dotlogics.com"
        },
        {
            "name": "Loffel",
            "email": "andrewcreym@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Loffel\\Grapesjs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Loffel\\Grapesjs\\GrapesjsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
